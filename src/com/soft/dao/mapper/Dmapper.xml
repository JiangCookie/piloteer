<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.soft.dao.DDao">
  
<select id="getbrand"  resultType="m">
  select * from cartype c,brand b where c.brandid=b.id
</select>

<select id="getmonth" resultType="m">
select month from month
</select>

<select id="getcartype" resultType="m">
select * from brand
</select>

<select id="getarea" resultType="m">
select * from area
</select>

<select id="getcon" resultType="m">
select * from condition
</select>
</mapper>